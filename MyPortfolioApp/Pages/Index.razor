@page "/"
@inject HttpClient Http
@inject ImageService ImageService
@inject ProjectService ProjectService
@inject IJSRuntime JSRuntime

<Header />
<Home />
<About Http=@Http />
<Quality />
<Skills />
<Projects ProjectService=@ProjectService/>
<Contact Http=@Http />
<Footer Http=@Http PrimaryColor="#4E567E" SecondaryColor="#D2F1E4" />

@code {
    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            await JSRuntime.InvokeVoidAsync("smoothScroll.initialize");
            await JSRuntime.InvokeVoidAsync("initializeTyped", "typed-element");
            //await JSRuntime.InvokeVoidAsync(".typed-text-output");
        }
    }
}